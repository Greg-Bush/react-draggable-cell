{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-airbnb"],
  "rules": {
    "indent": false,
    "ter-indent": false,
    "variable-name": false,
    "jsx-no-multiline-js": false,
    "object-literal-sort-keys": false,
    "prefer-array-literal": false,
    "quotemark": false,
    "no-increment-decrement": false,
    "import-name": false
  }
}